    "homepage": "https://github.com/frju365/libpsl-php",
    "license": "GPL-3.0-or-later",
    "version": "0.3-beta",
    "authors": [
        {
            "name": "Julien Gomes Dias",
            "email": "abld@abld.info",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-0": {
            "PSL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PSL\\Test\\": "test/"
        }
    }
